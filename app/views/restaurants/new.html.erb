<div class="wrapper">
  <div class="container">
    <div class="row">
      <h1>Novo Restaurant</h1>
      <%= simple_form_for (@restaurant) do |f| %>
      <%= f.input :name %>
      <%= f.input :address %>
      <%= f.input :phone_number %>
      <%= f.input :category %>
      <%= f.submit "Add new Restaurant", class:"btn btn-primary" %>
      <% end %>
    </div>
  </div>

</div>
